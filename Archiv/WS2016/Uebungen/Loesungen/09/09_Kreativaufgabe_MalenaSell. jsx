//@includepath "~/Documents/;%USERPROFILE%Documents";//@include "basiljs/basil.js";function draw() {  b.clear( b.doc() );  b.units(b.MM);  var stepSize = 10;    // gesamten Text der .csv-Datei auslesen  var weatherString = b.loadString("WetterHamburg.csv");  // ausgelesenen Text in einen Daten-Array umwandeln  b.CSV.delimiter(";");  var data = b.CSV.decode(weatherString);  b.size(stepSize * data.length, 300);  // zum Testen wird die Eigenschaft Luftfeuchtigkeit des ersten Datensets in die Konsole geschrieben  b.println(data[0].Luftfeuchtigkeit);  b.println(data[0].Windspitze);  for(var i = 0; i < data.length; i++) {    b.noStroke();    b.fill(b.color(0, 0, 255));    b.ellipse(i * stepSize, 150, -data[i].Luftfeuchtigkeit / 2, -data[i].Luftfeuchtigkeit / 2);    b.stroke(b.color(0));    b.line(i * stepSize, 300, i * stepSize, -data[i].Windspitze +160);    b.noStroke();    b.fill(b.color(0));    b.rect(i * stepSize, 200, stepSize, -data[i].TempTagesmittel);  }  for(var i = 0; i < data.length; i++) {    b.fill(b.color(255, 255, 0));    b.ellipse(i * stepSize, -data[i].Windspitze + 160, -data[i].Sonnenscheindauer * 2, -data[i].Sonnenscheindauer * 2);        b.fill(b.color(0));    b.rect(i * stepSize, 150, -data[i].Niederschlag, -data[i].Niederschlag);  }}}b.go();